{
  "name": "lightup-ai-backend",
  "version": "1.0.0",
  "description": "AI-driven intelligent learning platform backend",
  "main": "dist/app.js",
  "scripts": {
    "dev": "npm run db:generate && tsx watch src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "db:generate": "dotenv -e .env -- prisma generate --schema=../prisma/schema.prisma",
    "db:push": "dotenv -e .env -- prisma db push --schema=../prisma/schema.prisma",
    "db:migrate": "dotenv -e .env -- prisma migrate dev --schema=../prisma/schema.prisma",
    "db:reset": "dotenv -e .env -- prisma migrate reset --schema=../prisma/schema.prisma --force",
    "db:seed": "dotenv -e .env -- tsx src/scripts/seed.ts",
    "db:studio": "dotenv -e .env -- prisma studio --schema=../prisma/schema.prisma",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "ai",
    "learning",
    "education",
    "nodejs",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "axios": "^1.6.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.5",
    "@types/supertest": "^2.0.16",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "prisma": "^6.16.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}